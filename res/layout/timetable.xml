<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/ts1"
    tools:context="com.example.admin.attendanceassistant.MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Time Table"
        android:id="@+id/textView3"
        android:typeface="serif"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:typeface="serif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=" Faculty ID"
        android:id="@+id/textView4"
        android:layout_marginTop="32dp"

        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:textColor="@color/accent_material_light" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText9"
        android:width="150dp"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/weak"
        android:layout_alignStart="@+id/weak"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get"
        android:id="@+id/vtbt1"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/per"
        android:layout_centerHorizontal="true" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/weak"
        android:layout_below="@+id/editText9"
        android:layout_alignLeft="@+id/vtbt1"
        android:layout_alignStart="@+id/vtbt1"
        android:layout_marginTop="37dp" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/per"
        android:layout_below="@+id/weak"
        android:layout_alignLeft="@+id/weak"
        android:layout_alignStart="@+id/weak"
        android:layout_marginTop="35dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Weak Days"
        android:id="@+id/textView56"
        android:typeface="serif"
        android:layout_alignTop="@+id/weak"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_marginTop="10dp"
        android:textColor="@color/accent_material_light" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Period"
        android:id="@+id/textView57"
        android:typeface="serif"
        android:layout_alignTop="@+id/per"
        android:layout_alignLeft="@+id/textView56"
        android:layout_alignStart="@+id/textView56"
        android:layout_marginTop="10dp"
        android:textColor="@color/accent_material_light" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Your Period is"
        android:id="@+id/textView55"
        android:visibility="invisible"
        android:layout_marginTop="64dp"
        android:typeface="serif"
        android:layout_below="@+id/vtbt1"
        android:layout_alignLeft="@+id/textView57"
        android:layout_alignStart="@+id/textView57"
        android:textColor="@color/accent_material_light" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:visibility="invisible"
        android:id="@+id/textView58"
        android:typeface="serif"
        android:layout_alignTop="@+id/textView55"
        android:layout_alignLeft="@+id/editText9"
        android:layout_alignStart="@+id/editText9"
        android:textColor="@color/accent_material_light" />
</RelativeLayout>
